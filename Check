#Class 
import java.io.*;
import java.util.*;
public class Check {
public static void main(String args[])
{
Scanner sc=new Scanner(System.in);
int a;
System.out.println("Enter the Number:);
a=sc.nextInt();
if(a>0)
{
System.out.println("Given "+a+" is Positive");
}
else if(a<0)
{
System.out.println("Given "+a+" is Negative");
}
else
{
System.out.println("Given "+a+ is Zero");
}
}
}
